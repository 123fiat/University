+public class Main {
+    public static void main(String[] args) {
+
+        //System.out.println("Hello world!");
+        /*Дана строка “25 + 40 = 65”.  Замените символ “=” на слово “равно”.
+        Создать два метода. В первом используйте методы StringBuilder.insert(),
+        StringBuilder.deleteCharAt(). Во втором методы StringBuilder.replace().
+*/
+        StringBuilder first = new StringBuilder("25 + 40 = 65");
+        StringBuilder third = new StringBuilder("25 + 40 = 65");
+        StringBuilder second = new StringBuilder("equals");
+        changeChar(first, second);
+        changeChar2(third);
+    }
+    public static void changeChar(StringBuilder str, StringBuilder str1) {
+        int x = str.indexOf("=");
+        str.deleteCharAt(x);
+        str.insert(x, str1);
+        System.out.println(str);
+    }
+
+    public static void changeChar2(StringBuilder str) {
+        String str1 = "equals";
+        int x = str.indexOf("=");
+        str.replace(x, x + 1, str1);
+        System.out.println(str);
+    }
+}
